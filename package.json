{
  "name": "icns-preview",
  "displayName": "icns_preview",
  "icon": "preview.png",
  "description": "",
  "version": "0.0.1",
  "publisher": "d1y",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url":"https://github.com/icnskit/icns-preview"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:icns_preview.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "icns_preview.preview",
        "title": "Apple icns preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "icns_preview.preview",
          "group": "navigation",
          "when": "resourceExtname =~ /\\.icns$/"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/pngjs": "^6.0.1",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "pngjs": "^6.0.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@fiahfy/icns": "^0.0.7"
  }
}
